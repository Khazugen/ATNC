<UserControl x:Class="ATNC.Tunnel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ATNC"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Rectangle Fill="Silver" Grid.Column="1" Grid.RowSpan="9"/>

        <Rectangle Fill="Black"/>
        <Rectangle Fill="Yellow" Grid.Row="1"/>
        <Rectangle Fill="Black" Grid.Row="2"/>
        <Rectangle Fill="Black" Grid.Column="2"/>
        <Rectangle Fill="Yellow" Grid.Row="1" Grid.Column="2"/>
        <Rectangle Fill="Black" Grid.Row="2" Grid.Column="2"/>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="30"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="30"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="15"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
    </Grid>
</UserControl>

<Window x:Class="ATNC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ATNC"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <Grid x:Name="gr">
        <TextBox Width="200" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" TextChanged="T_Size_TextChanged"/>
        <local:CarGPX x:Name="car" Panel.ZIndex="9" Width="200" Height="100" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="30,350,0,0"/>
    </Grid>
</Window>
